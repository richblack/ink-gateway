{
  "name": "obsidian-ink-plugin",
  "version": "1.0.0",
  "type": "module",
  "description": "Obsidian plugin for Ink-Gateway integration with AI chat, semantic search, and content management",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "dev:enhanced": "node dev-helper.js",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern='src.*\\.test\\.ts$'",
    "test:integration": "jest --testPathPattern='tests/integration'",
    "test:performance": "jest --testPathPattern='tests/performance'",
    "test:load": "jest --testPathPattern='tests/load'",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:integration-new": "node scripts/run-integration-tests.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "build:release": "node scripts/build-release.js",
    "version:update": "node scripts/version-manager.js update",
    "version:release": "node scripts/version-manager.js release",
    "beta:setup": "node scripts/beta-testing-setup.js",
    "community:prep": "node scripts/community-plugin-prep.js"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ai",
    "semantic-search",
    "ink-gateway"
  ],
  "author": "Ink-Gateway Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "8.18.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}